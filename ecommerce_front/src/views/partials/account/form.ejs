<div class="row">
    <div class="col-xs-12">
        <h4>General</h4>
        <hr>
    </div>
</div>

<div class="form-group">
    <label for="name">Name</label>
    <input type="text" class="form-control" name="name" placeholder="Enter your full name">
</div>
<div class="form-group">
    <label for="email">Email</label>
    <input type="email" class="form-control" name="email" placeholder="Enter your e-mail address">
</div>

<div class="form-group">
    <label for="password">Password</label>
    <input type="password" class="form-control" name="password" placeholder="Enter your password">
</div>

<div class="row">
    <div class="col-xs-12">
        <h4>Personal</h4>
        <hr>
    </div>
</div>

<div class="form-group">
    <label for="birthday">Birthday</label>
    <input type="date" class="form-control" name="birthday" placeholder="XX/XX/XXXX">
</div>

<div class="form-group">
    <label for="birthday">ID (CPF)</label>
    <input type="date" class="form-control" name="cpf" placeholder="XXX.XXX.XXX-XX">
</div>

<div class="form-group">
    <label for="birthday">Cellphone</label>
    <input type="tel" class="form-control" name="cpf" placeholder="(XX) XXXXX-XXXX">
</div>

<div class="row">
    <div class="col-xs-12">
        <h4>Delivery</h4>
        <hr>
    </div>
</div>

<div class="form-group">
    <label for="birthday">Street</label>
    <input type="text" class="form-control" name="street" placeholder="Enter your street name">
</div>

<div class="form-group">
    <label for="birthday">Neighboor</label>
    <input type="text" class="form-control" name="neighboor" placeholder="Enter your street neighboor">
</div>

<div class="form-group">
    <label for="birthday">Number</label>
    <input type="number" class="form-control" name="number_delivery" placeholder="Enter your city name">
</div>

<div class="form-group">
    <label for="city">City</label>
    <input type="text" class="form-control" name="city" placeholder="Enter your city name">
</div>

<div class="form-group">
    <button type="submit" class="btn btn-blue-dark-default">Save</button>
</div>